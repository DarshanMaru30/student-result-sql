<!DOCTYPE html>
<html lang="en">

<head>
      <meta charset="UTF-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <title>Document</title>
</head>

<body>
      <table border="">
            <th>Name</th>
            <th>Std</th>
            <th>Division</th>
            <th>Roll No.</th>
            <th>Subject 1 Marks</th>
            <th>Subject 2 Marks</th>
            <th>Subject 3 Marks</th>
            <th>Subject 4 Marks</th>
            <th>Subject 5 Marks</th>
            <th>Subject 6 Marks</th>
            <th>Subject 7 Marks</th>
            <th>Total</th>
            <th>Minimum</th>
            <th>Maximum</th>
            <th>Result</th>
            <th>Edit</th>
            <%result.forEach(function(data){%>
                  <tr>
                        <td>
                              <%= data.name %>
                        </td>
                        <td>
                              <%= data.std %>
                        </td>
                        <td>
                              <%= data.division %>
                        </td>
                        <td>
                              <%= data.rollno %>
                        </td>
                        <td>
                              <%= data.s1 %>
                        </td>
                        <td>
                              <%= data.s2 %>
                        </td>
                        <td>
                              <%= data.s3 %>
                        </td>
                        <td>
                              <%= data.s4 %>
                        </td>
                        <td>
                              <%= data.s5 %>
                        </td>
                        <td>
                              <%= data.s6 %>
                        </td>
                        <td>
                              <%= data.s7 %>
                        </td>
                        <td>
                              <%= data.total %>
                        </td>
                        <td>
                              <%= data.minimum %>
                        </td>
                        <td>
                              <%= data.maximum %>
                        </td>
                        <td>
                              <%= data.resu %>
                        </td>
                        <td><a href="/staff/staffdashboard/viewresult/<%= data.id%>">Edit</a></td>
                  </tr>
                  <%})%>
      </table>
</body>

</html>